{
    "name": "helium/email-notifications",
	"description": "Custom Laravel email notifications",
    "type": "library",
    "license": "Apache-2.0",
	"version": "2.0.7",
    "authors": [
        {
            "name": "spencermerryman01",
            "email": "spencer@heliumservices.com"
        }
    ],
    "require": {
        "phpmailer/phpmailer": "^6.1",
        "league/oauth2-client": "^2.4",
        "swiftmailer/swiftmailer": "^6.2",
        "illuminate/support": "^7.8",
        "helium/facade-manager": "^1.0.3"
    },
	"autoload": {
		"psr-4": {
			"Helium\\EmailNotifications\\": "src/",
			"Helium\\EmailNotifications\\Tests\\": "tests/"
		}
	},
    "require-dev": {
        "orchestra/testbench": "^5.1"
    },
	"extra": {
		"laravel": {
			"providers": [
				"Helium\\EmailNotifications\\Providers\\EmailNotificationServiceProvider"
			],
			"aliases": {
				"EmailNotification": "Helium\\EmailNotifications\\EmailNotification"
			}
		}
	}
}
